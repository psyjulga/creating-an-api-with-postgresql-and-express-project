{
	"name": "storefront_backend",
	"version": "0.1.0",
	"description": "final udacity backend project",
	"main": "server.ts",
	"scripts": {
		"start": "tsc && node ./dist/server.js",
		"watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
		"test": "db-migrate --env test down -c5 && db-migrate --env test up && jest --runInBand --detectOpenHandles && db-migrate --env test down -c5 --config ./database.json",
		"migrate-up": "db-migrate up --config ./database.json",
		"migrate-down": "db-migrate down -c5 --config ./database.json",
		"tsc": "tsc",
		"db": "docker-compose up"
	},
	"author": "Ladybug, Udacity",
	"license": "ISC",
	"dependencies": {
		"bcrypt": "^5.0.1",
		"body-parser": "^1.19.0",
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"jsonwebtoken": "^8.5.1",
		"pg": "^8.5.1",
		"supertest": "^6.2.4",
		"typescript": "^4.1.3"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.9",
		"@types/jest": "^28.1.6",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/pg": "^7.14.7",
		"@types/sinon": "^10.0.13",
		"@types/supertest": "^2.0.12",
		"db-migrate": "^0.11.13",
		"db-migrate-pg": "^1.2.2",
		"dotenv": "^16.0.1",
		"jest": "^28.1.3",
		"ts-jest": "^28.0.7",
		"ts-node": "^9.1.1",
		"tsc-watch": "^4.2.9"
	}
}
